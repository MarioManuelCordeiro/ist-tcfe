* Title: Simple circuit
.options savecurrents

.include ../doc/circuit_data_4.cir

.ic v(6) = 8.403876e+00
.ic v(8) = 0

.control
*makes plots in color
set hcopypscolor=0
set color0=white
set color1=black
set color2=red
set color3=blue
set color4=violet
set color5=rgb:3/8/0
set color6=rgb:4/0/0
set units=degrees

op

echo "********************************************"
echo  "Transient analysis"
echo "********************************************"
tran 0.01m 20m
hardcopy forced.ps v(1) v(6) {v(6)-v(8)}
echo forced_FIG

echo "********************************************"
echo  "Frequency analysis"
echo "********************************************"
ac dec 100 0.1 1MEG
hardcopy acm.ps vdb(6) vdb(1) db(v(6)-v(8))
echo acm_FIG
hardcopy acp.ps vp(6) vp(1)
echo acp_FIG

quit
.endc

.end

